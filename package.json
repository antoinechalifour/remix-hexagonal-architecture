    "fp-ts": "2.11.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "0.1.13",
    "remix": "1.4.1",
    "reset.css": "2.0.2",
    "rxjs": "7.5.5",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.5",
    "@remix-run/dev": "1.3.5",
    "@testing-library/cypress": "8.0.2",
    "@types/express": "^4.17.13",
