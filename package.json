    "@types/jest": "27.4.0",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "@types/uuid": "8.3.3",
    "cypress": "9.2.1",
    "jest": "27.4.7",
    "prettier": "2.4.1",
    "prisma": "3.8.1",
    "start-server-and-test": "1.14.0",
    "ts-jest": "27.1.2",
    "typescript": "4.1.2"
  },
