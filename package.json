    "classnames": "2.3.1",
    "date-fns": "2.27.0",
    "fp-ts": "2.11.5",
    "joi": "17.5.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remix": "1.0.6",
    "reset.css": "2.0.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@remix-run/dev": "1.0.4",
